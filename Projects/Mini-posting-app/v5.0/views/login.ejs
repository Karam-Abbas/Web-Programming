<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <title>Document</title>
  </head>
  <body>
    <form action="/login" method="post">
      <div class="navbar">
        <h3>Login Account</h3>
        <div class="new">
          <a href="/" class="btn-blue">Create Account</a>
          <a href="/profile" class="btn-yellow">My Profile</a>
        </div>
      </div>
      <div class="main">
        <input type="email" name="email" class="same" placeholder="Email" />
        <input
          type="password"
          name="password"
          class="same"
          placeholder="Password"
        />
        <button type="submit" class="btn">Login</button>
      </div>
    </form>
  </body>
</html>
